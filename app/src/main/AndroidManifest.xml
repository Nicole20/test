<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nicole.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Colours"
            android:label="@string/colours" />
        <activity
            android:name=".Numbers"
            android:label="@string/numbers" />
        <activity
            android:name=".Music"
            android:label="Music" />
        <activity android:name=".Stuff"
            android:label="Stuff" />
        <activity android:name=".Editor"/>
        <provider
            android:authorities="com.example.nicole.test"
            android:name=".Data.provider"/>
    </application>

</manifest>